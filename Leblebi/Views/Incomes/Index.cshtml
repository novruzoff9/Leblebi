@model IEnumerable<Leblebi.Models.Income>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Gəlirlər</h1>

<p>
    <a asp-action="Create">Gəlir artır</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @* @Html.DisplayNameFor(model => model.Amount) *@
                Məbləğ
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.IncomeType) *@
                Ödəniş növü
            </th>
            <th>
                @* @Html.DisplayNameFor(model => model.IncomeDate) *@
                Tarix
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeDate)
            </td>
            <td>
                @* <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> | *@
                @* <a asp-action="Details" asp-route-id="@item.Id">Details</a> | *@
                @* <a asp-action="Delete" asp-route-id="@item.Id">Delete</a> *@
            </td>
        </tr>
}
    </tbody>
</table>
